  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Riz">
    <skos:definition xml:lang="fr">Plante cultivée pour ses grains comestibles par l'homme sous forme de riz ou de farine. Il est aussi utilisé pour la fabrication de boisson fermentée. Il existe plusieurs variétés de riz</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Riz">
    <skos:inScheme rdf:resource="http://ontology.inrae.fr/frenchcropusage/AgricultureRole"/>
