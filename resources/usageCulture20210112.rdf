  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Meteils">
    <skos:definition xml:lang="fr">Mélange de deux céréales pour l'alimentation animale. Culture composée de deux plantes le blé et le seigle pour alimentation humaine.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Meteils">
