  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Melilots">
    <skos:definition xml:lang="fr">Ensemble de plantes, les mélilots, dont plusieurs espèces sont cultivées:  le mélilot blanc et le mélilot jaune. Ces plantes sont utilisées comme fourrage, comme engrais vert, comme plante médicinale. Ce sont aussi des plantes mellifères.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Melilots">
    <skos:inScheme rdf:resource="http://ontology.inrae.fr/frenchcropusage/AgricultureRole"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Melilots">
    <skos:narrower rdf:resource="http://ontology.inrae.fr/frenchcropusage/Melilots_jaunes_engrais_verts"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Melilots">
