  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Jardins">
    <skos:definition xml:lang="fr">superficies de faible importance, destinées à la culture de produits pour la consommation du ménage: fruit, fleur, légume</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Jardins">
    <skos:inScheme rdf:resource="http://ontology.inrae.fr/frenchcropusage/AgricultureRole"/>
