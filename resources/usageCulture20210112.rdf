  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Millets">
    <skos:definition xml:lang="fr">Ensemble de plantes cultivées principalement pour ses grains comestibles par l'homme sous forme de céréales ou de farine.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Millets">
