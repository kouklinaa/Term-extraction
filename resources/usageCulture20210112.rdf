  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Ciboules">
    <skos:definition xml:lang="fr">Plantes potagères cultivées pour ses feuilles et sont bulbe comestibles par l'homme sous forme de légoume ou de condiment: la ciboule.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Ciboules">
    <skos:inScheme rdf:resource="http://ontology.inrae.fr/frenchcropusage/AgricultureRole"/>
