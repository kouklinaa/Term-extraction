  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Semences">
    <skos:definition xml:lang="fr">Ensemble de plantes cultivées pour produire des semences.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Semences">
    <skos:inScheme rdf:resource="http://ontology.inrae.fr/frenchcropusage/AgricultureRole"/>
