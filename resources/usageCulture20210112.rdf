  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Bananiers_musa">
    <skos:definition xml:lang="fr">Plante tropicale cultivée pour son fruit comestible par l'homme comme fruit sous le nom de banane</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Bananiers_musa">
    <skos:inScheme rdf:resource="http://ontology.inrae.fr/frenchcropusage/AgricultureRole"/>
