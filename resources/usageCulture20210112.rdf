  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Bredes">
    <skos:definition xml:lang="fr">Plante potagère cultivée pour ses feuilles comestibles par l'homme sous forme du légume brède mafane</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Bredes">
