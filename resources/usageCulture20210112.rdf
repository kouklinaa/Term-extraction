  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Haricots_demi_sec">
    <skos:definition xml:lang="fr">Plante potagère cultivée pour ses graines comestibles par l'homme sous forme de légume vert ou sec: le flageolet ou haricot à écosser ou haricot demi sec.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Haricots_demi_sec">
