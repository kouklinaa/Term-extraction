    <skos:definition xml:lang="fr">Plante potagère cultivée pour son fruit comestible par l'homme sous forme de légume ou de condiment: le piment.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontology.inrae.fr/frenchcropusage/Piments">
